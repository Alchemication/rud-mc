<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adam.rudmp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".CatalogueActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.adam.rudmp.SongActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.adam.rudmp.CatalogueActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.adam.rudmp.CatalogueActivity"/>

        </activity>

    </application>

</manifest>
